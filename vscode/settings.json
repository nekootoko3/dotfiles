  "editor.tabSize": 2,
  "editor.matchBrackets": false,
  "editor.folding": true,
  "editor.multiCursorModifier": "alt",
  "editor.formatOnSave": false,
  "editor.formatOnPaste": false,
  "editor.fontFamily": "Fira Mono",
  "editor.fontSize": 12,
  "editor.lineHeight": 21,
  "editor.fontWeight": "500",
  "editor.cursorStyle": "line",
