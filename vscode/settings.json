{
  "workbench.colorTheme": "Night Owl",
  "workbench.colorCustomizations": {
    "[Noctis Minimus]": {
      "tab.inactiveBackground": "#24333d"
    },
    "[Night Owl]": {
      "terminal.ansiBrightCyan": "#ffa7c4"
    }
  },
  "editor.tokenColorCustomizations": {
    "[Night Owl]": {
      "textMateRules": [
        {
          "name": "Number",
          "scope": ["constant.numeric", "constant.character.numeric"],
          "settings": {
            "foreground": "#ffa7c4",
            "fontStyle": ""
          }
        },
        {
          "name": "User-defined constant",
          "scope": ["constant.character", "constant.other"],
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "Constant Character Escape",
          "scope": "constant.character.escape",
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "Storage",
          "scope": [
            "storage",
            "meta.var.expr",
            "meta.class meta.method.declaration meta.var.expr storage.type.js",
            "storage.type.property.js",
            "storage.type.property.ts",
            "storage.type.property.tsx"
          ],
          "settings": {
            "foreground": "#ffa7c4",
            "fontStyle": "italic"
          }
        },
        {
          "name": "Storage type",
          "scope": "storage.type",
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "HTML Tag names",
          "scope": [
            "entity.name.tag",
            "meta.tag.other.html",
            "meta.tag.other.js",
            "meta.tag.other.tsx",
            "entity.name.tag.tsx",
            "entity.name.tag.js",
            "entity.name.tag",
            "meta.tag.js",
            "meta.tag.tsx",
            "meta.tag.html"
          ],
          "settings": {
            "foreground": "#ffa7c4",
            "fontStyle": ""
          }
        },
        {
          "name": "Library (function & constant)",
          "scope": ["support.function", "support.constant"],
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "Support Constant Property Value meta",
          "scope": "support.constant.meta.property-value",
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "Keyword Operator",
          "scope": "keyword.operator",
          "settings": {
            "foreground": "#ffa7c4",
            "fontStyle": ""
          }
        },
        {
          "name": "Keyword Operator Relational",
          "scope": "keyword.operator.relational",
          "settings": {
            "foreground": "#ffa7c4",
            "fontStyle": "italic"
          }
        },
        {
          "name": "Keyword Operator Assignment",
          "scope": "keyword.operator.assignment",
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "Keyword Operator Arithmetic",
          "scope": "keyword.operator.arithmetic",
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "Keyword Operator Bitwise",
          "scope": "keyword.operator.bitwise",
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "Keyword Operator Increment",
          "scope": "keyword.operator.increment",
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "Keyword Operator Ternary",
          "scope": "keyword.operator.ternary",
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "Variable Parameter Function",
          "scope": "variable.parameter.function",
          "settings": {
            "foreground": "#ffa7c4",
            "fontStyle": ""
          }
        },
        {
          "name": "Constant Other Color RGB Value Punctuation Definition Constant",
          "scope": "constant.other.color.rgb-value punctuation.definition.constant",
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "Keyword Control Operator",
          "scope": "keyword.control.operator",
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "Keyword Operator Comparison, imports, returns and Keyword Operator Ruby",
          "scope": [
            "keyword.operator.comparison",
            "keyword.control.flow.js",
            "keyword.control.flow.ts",
            "keyword.control.flow.tsx",
            "keyword.control.ruby",
            "keyword.control.module.ruby",
            "keyword.control.class.ruby",
            "keyword.control.def.ruby",
            "keyword.control.loop.js",
            "keyword.control.loop.ts",
            "keyword.control.import.js",
            "keyword.control.import.ts",
            "keyword.control.import.tsx",
            "keyword.control.from.js",
            "keyword.control.from.ts",
            "keyword.control.from.tsx"
          ],
          "settings": {
            "foreground": "#ffa7c4",
            "fontStyle": "italic"
          }
        },
        {
          "name": "Support Constant, `new` keyword, Special Method Keyword, `debugger`, other keywords",
          "scope": [
            "support.constant",
            "keyword.other.special-method",
            "keyword.other.new",
            "keyword.other.debugger",
            "keyword.control"
          ],
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "Support Constant, `new` keyword, Special Method Keyword, `debugger`, other keywords",
          "scope": [
            "support.constant",
            "keyword.other.special-method",
            "keyword.other.new",
            "keyword.other.debugger",
            "keyword.control"
          ],
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "Invalid Broken",
          "scope": "invalid.broken",
          "settings": {
            "foreground": "#020e14"
            // "background": "#ffa7c4"
          }
        },
        {
          "name": "Language Variable",
          "scope": "variable.language",
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "Support Variable Property",
          "scope": "support.variable.property",
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "CoffeeScript Assignments",
          "scope": "variable.assignment.coffee",
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "Wildcard(*) selector in Stylesheets",
          "scope": [
            "entity.name.tag.wildcard.css",
            "entity.name.tag.wildcard.less",
            "entity.name.tag.wildcard.scss",
            "entity.name.tag.wildcard.sass"
          ],
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "Attribute Name for CSS",
          "scope": [
            "meta.attribute-selector.css entity.other.attribute-name.attribute",
            "variable.other.readwrite.js"
          ],
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "JavaScript Method Declaration e.g. `constructor`",
          "scope": "meta.method.declaration storage.type.js",
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "Ruby Hashkeys",
          "scope": "constant.language.symbol.hashkey.ruby",
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "Ruby Symbols",
          "scope": "constant.language.symbol.ruby",
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "LESS Tag names",
          "scope": "entity.name.tag.less",
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "Attribute Name for LESS",
          "scope": "meta.attribute-selector.less entity.other.attribute-name.attribute",
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "Markdown MetaData Punctuation",
          "scope": ["punctuation.definition.metadata.markdown"],
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "Variables in SASS At-Rules",
          "scope": [
            "source.css.scss meta.at-rule variable",
            "source.css.sass meta.at-rule variable"
          ],
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "Attribute Name for SASS",
          "scope": [
            "meta.attribute-selector.scss entity.other.attribute-name.attribute",
            "meta.attribute-selector.sass entity.other.attribute-name.attribute"
          ],
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "Tag names in SASS",
          "scope": ["entity.name.tag.scss", "entity.name.tag.sass"],
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "TypeScript[React] Node Classes",
          "scope": ["support.class.node.ts", "support.class.node.tsx"],
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "YAML Entity Name Tags",
          "scope": "entity.name.tag.yaml",
          "settings": {
            "foreground": "#ffa7c4"
          }
        },
        {
          "name": "Support Class Component",
          "scope": [
            "support.class.component.js",
            "support.class.component.tsx"
          ],
          "settings": {
            "foreground": "#ffa7c4",
            "fontStyle": ""
          }
        }
      ]
    }
  },
  "workbench.list.automaticKeyboardNavigation": false,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.view.alwaysShowHeaderActions": true,
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.fontAliasing": "auto",
  "workbench.editor.showTabs": true,
  "workbench.editor.labelFormat": "medium",
  "workbench.settings.editor": "json",
  "workbench.settings.useSplitJSON": true,
  "workbench.editor.showIcons": true,
  "workbench.statusBar.visible": true,
  "workbench.tree.indent": 16,
